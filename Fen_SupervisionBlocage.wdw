  -
    name : BTN_Vérifier
    identifier : 0x2567024d774a6538
    internal_properties : 
    properties :
     x : 12
     y : 158
     width : 125
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         si pas ouinon(non,<§$0001§>) alors retour   
         //2001:En cas de fermeture anormale d'une application, le moteur HFSQL débloque automatiquement les enregistrements...
         
         // Cette routine vérifie si aucune info n'est restée mémorisée 
         // dans le fichier "Verrou" alors que le déblocage a bien eu lieu 
         // (Fermeture anormale de l'application)
         
         Fintimersys(1)
         
         // Parcours séquentiel du fichier "Verrou"
         i est un entier  
         nNbCorrections est un entier  
         boucle
         	i++
         	boucle
         		Hlit(VERROU,i)
         		si HErreurBlocage() alors
         			multitache(200) 	//	Ré-essai 2 s. plus tard
         		sinon
         			sortir
         		FIN
         	FIN
         	si HEnDehors() alors sortir
         	si (HEtat()=HEtatActif) alors
         		// Test si l'enregistrement est effectivement bloqué
         		hlit(VERROU.FICHIER,VERROU.ENREG,HBlocageEcriture)
         		si pas HErreurBlocage() alors
         			// Supprime les informations du fichier "Verrou"
         			boucle
         				HSupprime(VERROU)
         				si HErreurBlocage() alors
         					multitache(200) 	//	Ré-essai 2 s. plus tard
         				sinon
         					sortir
         				FIN
         			fin
         			nNbCorrections++
         		fin
         	fin
         fin
         TableAffiche(TABLE,taDebut)
         timersys(Rafraichir,50,1)
         
         selon nNbCorrections
         	cas 0
         		info(<§$000b§>,...
         		<§$000c§>)
         	cas 1
         		info(<§$000d§>,...
         		<§$000e§>)
         	autres cas
         		info(nNbCorrections+<§$000f§>,...
         		<§$0010§>)
         fin
         
        type : 18
      -
        type : 161
        enabled : false
  -
    name : BTN_Débloquer
    identifier : 0x2567024d774b6538
    internal_properties : 
    properties :
     x : 12
     y : 222
     width : 125
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : CAAAAAgAAABPKR6VVcL8snuAm24Yo8BZvyFEEPjRoyeTfu/EJLHnB0WobozrdMmhbC8fEDQChS5jnvJ8AHhQghhHDrNi7ozuRy9Sof+iHg6uqJiymODmNMWH1onsOeyNWip7AgFn1X6/s61fXL8WjkCSs+g=
        code : |1+
         si TABLE..vide alors
         	bip
         	retour
         FIN
         
         si pas ouinon(non,<§$0002§>) alors retour
         
         
         fintimersys(1)
         
         // Test si l'enregistrement est effectivement bloqué
         hlit(VERROU.FICHIER,VERROU.ENREG,HBlocageEcriture)
         si pas HErreurBlocage() alors
         	// Supprime les informations du fichier "Verrou"
         	boucle
         		HSupprime(VERROU)
         		si HErreurBlocage() alors
         			multitache(200) 	//	Ré-essai 2 s. plus tard
         		sinon
         			sortir
         		FIN
         	fin
         sinon
         	// Test si l'enregistrement existe toujours
         	hlit(VERROU.FICHIER,VERROU.ENREG)
         	si (HEtat()=HEtatActif)
         		// Force le flag pour demander à l'application correspondante de se fermer
         		// (et donc de débloquer l'enregistrement correspondant)
         		MsgàEnvoyer est une chaine 
         		MsgàEnvoyer = ouvre(FEN_SaisieMessage,	<§$0011§>,VERROU.UTILISATEUR)
         		si MsgàEnvoyer<>"" alors
         			VERROU.ENVOIMSG = 9
         			VERROU.MSG = MsgàEnvoyer
         			VERROU.AUTEURMSG = reseauutilisateur()
         			boucle
         				Hmodifie(VERROU)
         				si HErreurBlocage() alors
         					multitache(200) 	//	Ré-essai 2 s. plus tard
         				sinon
         					sortir
         				FIN
         			fin
             	fin
         	fin
         fin
         timersys(Rafraichir,50,1)
         
        type : 18
      -
        type : 161
        enabled : false
  -
    name : BTN_Libérer
    identifier : 0x2567024d774c6538
    internal_properties : 
    properties :
     x : 12
     y : 190
     width : 125
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         si TABLE..vide alors
         	bip
         	retour
         FIN
         
         	
         si ouinon(non,Chaineconstruit(<§$0012§> + rc +...
         <§$0013§>,VERROU.UTILISATEUR,COL_Durée,VERROU.UTILISATEUR)) alors
         	fintimersys(1)
         	// Message pour l'utilisateur en train de modifier l'enregistrement
         	sMsgàEnvoyer est une chaine 
         	sMsgàEnvoyer = ouvre(FEN_SaisieMessage,	<§$0014§>,VERROU.UTILISATEUR)
         	si sMsgàEnvoyer<>"" alors
         		// Message pour l'utilisateur en train de modifier l'enregistrement
         		VERROU.ENVOIMSG = vrai
         		VERROU.MSG = sMsgàEnvoyer
         		VERROU.AUTEURMSG = reseauutilisateur()
         		boucle
         			hmodifie(VERROU)
         			si HErreurBlocage() alors
         				multitache(200) 	//	Ré-essai 2 s. plus tard
         			sinon
         				sortir
         			FIN
         		fin
         		
         	fin
         	si herreur(hErrEnCours) = 0 ALORS
         		timersys(Rafraichir,50,1)		
         	sinon
         		// Une erreur autre qu'une erreur de blocage est survenue
         		// Affichage d'un message d'erreur
         		erreur(<§$0016§>,<§$0017§> + herreurinfo())
         	FIN
         fin
         
        type : 18
      -
        type : 161
        enabled : false
