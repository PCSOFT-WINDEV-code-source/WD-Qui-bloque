     name : CalculDuree
     internal_properties : CAAAAAgAAADkNP5d2hLQKr7pikhgXI8/ya6ffSP+b2TikzOzb8vXzUQvdpoa34wJvFw2CIyZd3zLgwTUxTl2sDObZqBnrTahCz87bqJIveo+JqwGxtIpfsR3B5rJBfJPL9Y8S1IVY86gTxa/qhVRY0TJkvlyLzo/8oTzXlecmiXGreW89IM78M4FCPuvG4NWhYCiEye9dtoVVOid
     procedure_id : 1542203927181092152
     type_code : 14
     code : |1+
      Procédure CalculDuree(i)
      
      // Calcule la durée du blocage de la ligne demandée
      
      quand exception
          // Ignore l'erreur si date invalide
          COL_Durée[i] = "?"
      	retour
      fin
      
      // Calcul de la durée
      sDiff est une chaîne = DateHeureDifférence(COL_DateDébut[i]+COL_HeureDébut[i],Datedujour()+Maintenant()[[1 à 6]])
      // Affichage temps écoulé
      sTxtAffiché est une chaine
      
      si val(sDiff[[2 à 8]])>0 alors sTxtAffiché += val(sDiff[[2 à 8]])+" j. " // Jours  (même si improbable)
      
      si sTxtAffiché~="" 	// < 1 jour => test si heures
      	si val(Milieu(sDiff,9,2))>0 alors sTxtAffiché += Milieu(sDiff,9,2)+" h " // Heures (possible ?)
      sinon				// > 1 jour => heures tjs affichés
      	sTxtAffiché += Milieu(sDiff,9,2)+" h " // Heures (possible ?)		
      fin
      
      sTxtAffiché +=  Milieu(sDiff,11,2)+" ' " + Milieu(sDiff,13,2) + " '' "
      COL_Durée[i] = sTxtAffiché
      
     type : 458752
   -
     name : Rafraichir
     procedure_id : 1542203927181157688
     type_code : 14
     code : |1+
      Procédure Rafraichir()
      
      // Pour détection automatique des modifications faites sur les autres postes.
      
      Fintimersys(1)
      si gnVersionFichier <> hversion(VERROU) alors
      	// Modification faîte sur un autre poste
      	gnVersionFichier = hversion(VERROU)
      	TableAffiche(TABLE,taDebut)
      	si table..vide alors table..bulle=""
      FIN
      timersys(Rafraichir,50,1)
      
      ReafficheDurees()
      
     type : 458752
